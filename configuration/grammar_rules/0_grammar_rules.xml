<grammar_rules>
  <grammar_rule left_child="definite_article" right_child="singular_noun" superordinate="definite_article_singular"/>
  <grammar_rule left_child="definite_article" right_child="plural_noun" superordinate="definite_article_plural"/>
  <grammar_rule left_child="indefinite_article" right_child="singular_noun" superordinate="indefinite_article_singular"/>
  <grammar_rule left_child="indefinite_article_singular" superordinate="article_singular"/>
  <grammar_rule left_child="definite_article_singular" superordinate="article_singular"/>
  
  <!--3rd person singular begin-->
  <grammar_rule left_child="article_singular" superordinate="3rdPersSingSubjOrObjEnumEle"/>
  <grammar_rule left_child="he" superordinate="3rdPersSingSubjOrObjEnumEle"/>
  <grammar_rule left_child="she" superordinate="3rdPersSingSubjOrObjEnumEle"/>
  <grammar_rule left_child="it" superordinate="3rdPersSingSubjOrObjEnumEle"/>
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="and" superordinate="3rdPersSingSubjOrObjEnumEle_AND"/>
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle_AND" right_child="3rdPersSingSubjOrObjEnumEle" superordinate="3rdPersPlurSubjOrObjEnumEle"/>
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle_AND" right_child="3rdPersPlurSubjOrObjEnumEle" superordinate="3rdPersPlurSubjOrObjEnumEle"/>
  <!--grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="MainVerb3rdPersSingPres1Obj" superordinate="3rdPersSingClause1Obj"/-->
  <grammar_rule left_child="MainVerb3rdPersSingPres1Obj" superordinate="Middle3rdPersSing1Obj"/>
  <grammar_rule left_child="adverb" right_child="MainVerb3rdPersSingPres1Obj" superordinate="Middle3rdPersSing1Obj"/>
  <grammar_rule left_child="adverb" right_child="MainVerb3rdPersSingPres0Obj" superordinate="Middle3rdPersSing0Obj"/>
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="Middle3rdPersSing1Obj" superordinate="3rdPersSingClause1Obj"/>
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="Middle3rdPersSing0Obj" superordinate="3rdPersSingClause0Obj"/>
  
  <grammar_rule left_child="3rdPersSingClause1Obj" superordinate="clause1Obj"/>
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" superordinate="SubjOrObjEnumEle"/>
  
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="MainVerb3rdPersSingPres0Obj" superordinate="3rdPersSingClauseAllows0Obj"/>
  <!--e.g. "still burns"  
     adverb_before_finite_verb: "still" 
	 3rdPersSingSubjOrObjEnumEle: ??
    -->
  <grammar_rule left_child="adverb_before_finite_verb" right_child="3rdPersSingSubjOrObjEnumEle" superordinate="3rdPersSingPres0ObjMiddle" />
  
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="3rdPersSingPres0ObjMiddle" superordinate="3rdPersSingClauseAllows0Obj"/>
  
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="MainVerb3rdPersSingPres1Obj" superordinate="3rdPersSingClauseAllows1Obj"/>
  <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="MainVerb3rdPersSingPres2Obj" superordinate="3rdPersSingClauseAllows2Obj"/>
  
  <!--relative clause -->
    <!-- can be cascaded (->recursive) : -->
    <!--the car               the man               you like      likes is pretty.
	    <sub or obj enum ele> <sub or obj enum ele> <rel. clause>
		                      +++++++++++relative clause+++++++++++++++
							  
	("das Auto, dass der Mann, den du magst, mag, ist schön".)-->
	
   <!-- e.g.: "the car he likes":  
     3rdPersSingSubjOrObjEnumEle: "the car"  
	 3rdPersSingClauseAllows1Obj: "he likes" (needs 1 object: "the car")  
    -->
   <grammar_rule left_child="3rdPersSingSubjOrObjEnumEle" right_child="3rdPersSingClauseAllows1Obj" superordinate="3rdPersSingEnumEleWithRelClause"/>
   
   <!-- e.g.: "the car he likes works.":  
     3rdPersSingSubjOrObjEnumEle: "the car"  
	 3rdPersSingClauseAllows1Obj: "he likes" (needs 1 object: "the car")  
     -->
   <grammar_rule left_child="3rdPersSingEnumEleWithRelClause" right_child="MainVerb3rdPersSingPres0Obj" superordinate="3rdPersSingEnumEleWithRelClauseAllows0Obj"/>
   
   <!--to enable casceded rel clauses: 
        the car               the man               he likes      likes is pretty.
	    <sub or obj enum ele> <sub or obj enum ele> <rel. clause>
		                      +++++++++++relative clause+++++++++++++++
		3rdPersSingEnumEleWithRelClause: "the man he likes"
		MainVerb3rdPersSingPres1Obj: "likes"
     -->
   <grammar_rule left_child="3rdPersSingEnumEleWithRelClause" right_child="MainVerb3rdPersSingPres1Obj" superordinate="3rdPersSingClauseAllows1Obj"/>
    
  <!--end of relative clauses -->
  
  <!--3rd person singular end-->

  <!--3rd person plural begin-->
  <grammar_rule left_child="they" superordinate="3rdPersPlurSubjOrObjEnumEle"/>
  <grammar_rule left_child="3rdPersPlurSubjOrObjEnumEle" right_child="and" superordinate="3rdPersPlurSubjOrObjEnumEle_AND"/>
  <grammar_rule left_child="3rdPersPlurSubjOrObjEnumEle_AND" right_child="3rdPersPlurSubjOrObjEnumEle" superordinate="3rdPersPlurSubjOrObjEnumEle"/>
  <grammar_rule left_child="3rdPersPlurSubjOrObjEnumEle_AND" right_child="3rdPersSingSubjOrObjEnumEle" superordinate="3rdPersPlurSubjOrObjEnumEle"/>
  <grammar_rule left_child="3rdPersPlurSubjOrObjEnumEle_AND" right_child="I" superordinate="we"/>
  <grammar_rule left_child="plural_noun" superordinate="plural_noun_superclass"/>
  <grammar_rule left_child="definite_article_plural" superordinate="plural_noun_superclass"/>
  <grammar_rule left_child="plural_noun_superclass" superordinate="3rdPersPlurSubjOrObjEnumEle"/>
  <grammar_rule left_child="3rdPersPlurSubjOrObjEnumEle" superordinate="SubjOrObjEnumEle"/>
  <grammar_rule left_child="3rdPersPlurSubjOrObjEnumEle" right_child="mainVerbInf0Obj" superordinate="3rdPersPlurClauseAllows0Obj"/>
  <grammar_rule left_child="3rdPersPlurSubjOrObjEnumEle" right_child="mainVerbInf1Obj" superordinate="3rdPersPlurClauseAllows1Obj"/>
  <grammar_rule left_child="3rdPersPlurSubjOrObjEnumEle" right_child="mainVerbInf2Obj" superordinate="3rdPersPlurClauseAllows2Obj"/>
  <!--3rd person plural end-->
  
  <!--objects-->
  <grammar_rule left_child="SubjOrObjEnumEle" superordinate="ObjEnumEle"/>
  <grammar_rule left_child="pers_pron_obj" superordinate="ObjEnumEle"/>
  <grammar_rule left_child="ObjEnumEle"  right_child="and" superordinate="ObjEnumEle_and"/>
  <!--  e.g. for "the car and the cars and" -->
  <grammar_rule left_child="ObjEnumEle_and"  right_child="ObjEnumEle_and" superordinate="ObjEnumEle_and"/>
  <grammar_rule left_child="ObjEnumEle_and"  right_child="ObjEnumEle" superordinate="ObjEnumEle"/>
  <grammar_rule left_child="ObjEnumEle"  superordinate="obj"/>
  
  <!--objects end-->
  
  <!-- clauses with object(s) -->
  <grammar_rule left_child="3rdPersPlurClauseAllows1Obj" right_child="3rdPersPlurObj" superordinate="3rdPersPlurClauseWith1Obj"/>
  <grammar_rule left_child="3rdPersPlurClauseAllows1Obj" right_child="obj" superordinate="3rdPersPlurClauseWith1Obj"/>
  <!-- clauses with object(s) -->
    
  <grammar_rule left_child="SubjOrObjEnumEle" superordinate="obj"/>
  
  <grammar_rule left_child="clause1Obj" right_child="obj" superordinate="clauseWith1Obj"/>
  
  <grammar_rule left_child="clause" right_child="obj" superordinate="clause_with_obj"/>
  
  <grammar_rule left_child="relative_pronoun" right_child="3rdPersSingClauseAllows1Obj" superordinate="relative_clause"/>
</grammar_rules>