<translation_rules>
  <!--personal pronouns begin-->
  <translation_rule 
    syntax_tree_path="I"
    translation="ich">
  </translation_rule>  
  <!--<translation_rule 
    syntax_tree_path="you"
    translation="du">
  </translation_rule>-->
  <translation_rule 
    syntax_tree_path="he"
    translation="er">
  </translation_rule>  
  <translation_rule 
    syntax_tree_path="she"
    translation="sie">
  </translation_rule>  
  <translation_rule 
    syntax_tree_path="it"
    translation="es">
  </translation_rule>  
  <translation_rule 
    syntax_tree_path="we"
    translation="wir">
  </translation_rule>  
  <!--<translation_rule 
    syntax_tree_path="you"
    translation="ihr">
  </translation_rule>-->
  <translation_rule 
    syntax_tree_path="they"
    translation="sie">
  </translation_rule>  
</translation_rules>