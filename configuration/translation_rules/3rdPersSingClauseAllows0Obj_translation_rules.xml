<translation_rules>

  <!-- _subject_ enumeration element -->
  <translation_rule 
    syntax_tree_path="*.3rdPersSingClauseAllows0Obj.*.definite_article_singular.singular_noun"
    attribute_name="German_singular"
  />
  <!-- _subject_'s definite article -->
  <translation_rule 
    syntax_tree_path="*.3rdPersSingClauseAllows0Obj.*.definite_article_singular.definite_article"
    translation="der">
    <condition syntax_tree_path="definite_article_singular.singular_noun"
      attribute_name="German_noun_article"
      byte_attribute_value="0"
    />
  </translation_rule>
  <!-- _subject_'s definite article -->
  <translation_rule 
    syntax_tree_path="*.3rdPersSingClauseAllows0Obj.*.definite_article_singular.definite_article"
    translation="das">
    <condition syntax_tree_path="definite_article_singular.singular_noun"
      attribute_name="German_noun_article"
      byte_attribute_value="2"
    />
  </translation_rule>

  <translation_rule 
    syntax_tree_path="MainVerb3rdPersSingPres0Obj"
    attribute_name="GerMainVerb0Obj3rdPersSing" 
  />
  <!--<translation_rule 
    syntax_tree_path="MainVerb3rdPersSingPres0Obj"
    translation="sich">
    <condition syntax_tree_path="MainVerb3rdPersSingPres0Obj"
     attribute_name="reflexive"
      byte_attribute_value="1"
    />
  </translation_rule>-->
    
  <!-- e.g. the sheep trusts the man." -> ...vertraut DEM Mann-->
  <translation_rule 
    syntax_tree_path="obj.*.definite_article_singular.definite_article"
    translation="dem">
    <condition syntax_tree_path="definite_article_singular.singular_noun"
      attribute_name="German_noun_article"
      byte_attribute_value="0"
    />
    <!--grammatical_case=3 <=> dative verb-->
    <condition syntax_tree_path="clauseWith1Obj.clause1Obj.3rdPersSingClause1Obj.MainVerb3rdPersSingPres1Obj"
      attribute_name="grammatical_case"
      byte_attribute_value="3"
    />
  </translation_rule>
  <!-- e.g. the sheep trusts the child." -> ...vertraut DEM Kind-->
  <translation_rule 
    syntax_tree_path="obj.*.definite_article_singular.definite_article"
    translation="dem">
    <condition syntax_tree_path="definite_article_singular.singular_noun"
      attribute_name="German_noun_article"
      byte_attribute_value="2"
    />
    <!--grammatical_case=3 <=> dative verb-->
    <condition syntax_tree_path="clauseWith1Obj.clause1Obj.3rdPersSingClause1Obj.MainVerb3rdPersSingPres1Obj"
      attribute_name="grammatical_case"
      byte_attribute_value="3"
    />
  </translation_rule>
 
  <!--Das Kind vertraut >>den<< Kindern-->
  <translation_rule 
    syntax_tree_path="clauseWith1Obj.obj.*.definite_article_plural.definite_article"
    translation="den">
    <!--grammatical_case=3 <=> dative verb-->
    <condition syntax_tree_path="clauseWith1Obj.clause1Obj.3rdPersSingClause1Obj.MainVerb3rdPersSingPres1Obj"
      attribute_name="grammatical_case"
      byte_attribute_value="3"
    />
  </translation_rule>
  <!--translation_rule 
    syntax_tree_path="3rdPersSingClauseAllows0Obj.mainVerbInf0Obj"
    attribute_name="Ger0ObjMainVerb3rdPersSing" 
  /-->
</translation_rules>