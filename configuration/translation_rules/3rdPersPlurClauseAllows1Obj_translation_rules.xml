<translation_rules>

  <!-- _subject_ enumeration element -->
  <translation_rule 
    syntax_tree_path="*.3rdPersPlurClauseAllows1Obj.*.definite_article_plural.plural_noun"
    attribute_name="German_plural"
  />
  
  <!-- <translation_rule 
    syntax_tree_path="*.MainVerb3rdPersSingPres0Obj"
    attribute_name="GerMainVerb0Obj3rdPersSing" 
  /> -->

  <!-- _subject_'s definite article 
     Differentiate "the sheep eat" -> "die Schafe essen." from "the sheep eats" -> "das Schaf isst." -->
  <translation_rule 
	translation="die"
    syntax_tree_path="*.3rdPersPlurClauseAllows1Obj.*.definite_article_plural.definite_article"
  />

  <!--translation of predicate/ (finite) verb-->
  <translation_rule 
    syntax_tree_path="3rdPersPlurClauseAllows1Obj.mainVerbInf1Obj"
    attribute_name="GerMainVerb0Obj3rdPersPlurPres" 
  />
  <translation_rule 
    syntax_tree_path="*.3rdPersPlurClauseAllows1Obj.mainVerbInf1Obj"
    attribute_name="GerMainVerb1Obj3rdPersPlurPres" 
  />
  
  <!--index 6 is finite verb form for 3rd person plural -->
  <!--translation_rule 
    syntax_tree_path="3rdPersPlurClauseAllows0Obj.mainVerbInf0Obj"
    attribute_name="Ger0ObjMainVerb3rdPersPlur" 
    byte_attribute_value="6"
  /-->
  <translation_rule 
    syntax_tree_path="3rdPersPlurClauseAllows1Obj.mainVerbInf1Obj"
    attribute_name="GerMainVerb1Obj3rdPersPlurPres" 
    byte_attribute_value="6"
  />
  <!--translation_rule 
    syntax_tree_path="3rdPersPlurClauseAllows2Obj.mainVerbInf2Obj"
    attribute_name="Ger2ObjMainVerb3rdPersPlur" 
    byte_attribute_value="6"
  /-->
  
  <!--article translation begin-->
  <!--<translation_rule 
    syntax_tree_path="definite_article_plural.definite_article"
    translation="die">
  </translation_rule>-->
    
</translation_rules>